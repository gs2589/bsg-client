{{outlet}}
<h1>{{model.name}}</h1>





<div><br>
<h2>{{model.name}}'s Teams:</h2>
<ul>
{{#each model.teams as |team|}}
<li>{{#link-to 'teams.team' team}}{{team.name}}{{/link-to}}</li>
{{/each}}
</ul>
</div>

<div><br>
<h2> Rides {{model.name}} is going on: </h2>
<ul>
{{#each model.rides as |ride|}}
<li>{{#link-to 'rides.ride' ride}}{{ride.name}}{{/link-to}}</li>
{{/each}}
</ul>
</div>